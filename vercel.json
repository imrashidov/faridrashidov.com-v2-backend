{
  "version": 2,
  "builds": [
    {
      "src": "/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/csses", "method": "GET", "dest": "/routes/csses.js" },
    { "src": "/api/projects", "method": "GET", "dest": "/routes/projects.js" },
    { "src": "/api/designs", "method": "GET", "dest": "/routes/designs.js" },
    { "src": "/(.*)", "dest": "server.js" }
  ]
}
